FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 0 0 1 0 0 0];

internalField       uniform 558.638;

boundaryField
{
    "(bottom|top)"
    {
        type            zeroGradient;
    }
    
    right
    {
        type            robinTemperature;
        h               uniform 400.0;
        Tinf            uniform 298.0;
        value           $internalField;
    }
    
    left     
    {
        type            symmetryPlane;
    }
}

