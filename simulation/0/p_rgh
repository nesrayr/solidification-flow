FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 0;

boundaryField
{
    bottom
    {
        type            fixedFluxPressure;
        gradient        nonuniform List<scalar> 0;
        value           nonuniform List<scalar> 0;
    }
    top
    {
        type            fixedFluxPressure;
        gradient        nonuniform List<scalar> 0;
        value           nonuniform List<scalar> 0;
    }
    left
    {
        type            symmetryPlane;
    }
    right
    {
        type            fixedFluxPressure;
        gradient        nonuniform List<scalar> 0;
        value           nonuniform List<scalar> 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


